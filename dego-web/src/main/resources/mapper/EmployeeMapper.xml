<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dego.mapper.EmployeeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dego.entity.po.Employee">
        <result column="id" property="id" />
        <result column="account" property="account" />
        <result column="password" property="password" />
        <result column="real_name" property="realName" />
        <result column="mobile" property="mobile" />
        <result column="email" property="email" />
        <result column="picture" property="picture" />
        <result column="sex" property="sex" />
        <result column="create_time" property="createTime" />
        <result column="create_user" property="createUser" />
        <result column="remark" property="remark" />
        <result column="is_delete" property="isDelete" />
        <result column="is_init_password" property="isInitPassword" />
        <result column="is_dev" property="isDev" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        account, password, real_name, mobile, email, picture, sex, create_time, create_user, remark, is_delete, is_init_password, is_dev
    </sql>

</mapper>
